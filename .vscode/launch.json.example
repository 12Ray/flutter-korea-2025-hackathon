{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "MetaNote (Dev)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--dart-define=GEMINI_API_KEY=your_gemini_api_key_here"
      ]
    },
    {
      "name": "MetaNote (Release)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "release",
      "args": [
        "--dart-define=GEMINI_API_KEY=your_gemini_api_key_here"
      ]
    },
    {
      "name": "MetaNote (Web)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "deviceId": "chrome",
      "args": [
        "--dart-define=GEMINI_API_KEY=your_gemini_api_key_here"
      ]
    }
  ]
}